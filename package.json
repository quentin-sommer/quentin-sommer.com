  "dependencies": {
    "next": "beta",
    "per-env": "^1.0.2",
    "prettier": "^1.4.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-typography": "^0.15.10",
    "styled-components": "^2.1.0",
    "typography": "^0.15.10"
